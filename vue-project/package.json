{
  "name": "labmanger",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build ",
    "preview": "vite preview",
    "electron:serve": "electron .",
    "build-electron-win": "electron-builder --dir --win --ia32",
    "app:dist": "electron-builder"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^20.3.12",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-vue-components": "^28.1.0",
    "vite": "^6.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.0"
  },
  "build": {
    "productName": "LabManger",
    "appId": "LabManger",
    "extraFiles": [
      "./conf.txt"
    ],
    "files": [
      "./dist",
      "./main.js",
      "./preload.js"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "electron-dist"
    },
    "win": {
      "icon": "./icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  }
}
